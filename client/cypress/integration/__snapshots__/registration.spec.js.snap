exports[`Registration Page > should display registration form #0`] = `
<main data-testid="registration-page" class="auth__register_container">
  <section class="form-section">
    <header class="header">
      <img
        src="data:image/png;base64,"
        class="logo"
      />
      <p>Already a member? <a href="/login">Log In</a></p>
    </header>
    <article class="form-container">
      <header class="form-header"><h1>Get started for free</h1></header>
      <p class="divider">Register With</p>
      <section class="oauth-button-group">
        <a class="btn btn-white" href="/"
          ><img
            class="svg-icon"
            src="/static/media/github.1b510489.svg"
          />Github</a
        ><a class="btn btn-white" href="/"
          ><img
            class="svg-icon"
            src="/static/media/google.6bb8c994.svg"
          />Google</a
        >
      </section>
      <p class="divider">Or</p>
      <form class="form">
        <div class="form-group">
          <label>Full Name</label
          ><input class="form-input" id="fullName" type="text" required="" />
        </div>
        <div class="form-group">
          <label>Email address</label
          ><input class="form-input" id="email" type="email" required="" />
        </div>
        <div class="form-group">
          <label>Password</label
          ><input
            class="form-input"
            id="password"
            type="password"
            required=""
          />
        </div>
        <button type="submit" class="btn btn-primary">Create account</button>
      </form>
      <p class="term-and-conditions">
        By signing up you agree to our <a href="/terms">Terms of service</a> and
        acknowledge our <a href="/privacy-policy">Privacy Policy</a>
      </p>
    </article>
  </section>
  <aside class="about-section">
    <img src="/static/media/auth-svg1.a68e392c.svg" class="svg-icon icon1" />
    <section class="about-information">
      <h1>The new standard in content management</h1>
      <p>
        Finally, the freedom to use your favorite tools and frameworks without
        CMS technical debt.
      </p>
    </section>
    <img src="/static/media/auth-svg2.0ca4be78.svg" class="svg-icon icon2" />
  </aside>
</main>
`;

